{
  "name": "my-frontend",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --config ./config/astro.config.mjs",
    "build": "astro build --config ./config/astro.config.mjs",
    "preview": "astro preview --config ./config/astro.config.mjs",
    "astro": "astro",
    "test": "vitest --config ./config/vitest.config.js",
    "test:ui": "vitest --ui --config ./config/vitest.config.js",
    "lint": "eslint . --config ./config/eslint.config.js",
    "lint:fix": "eslint . --fix --config ./config/eslint.config.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "astro check --config ./config/astro.config.mjs"
  },
  "dependencies": {
    "@astrojs/node": "^9.0.0",
    "@tailwindcss/vite": "4.1.18",
    "astro": "5.16.6",
    "tailwindcss": "4.1.18"
  },
  "devDependencies": {
    "@astrojs/check": "0.9.4",
    "@typescript-eslint/eslint-plugin": "8.19.1",
    "@typescript-eslint/parser": "8.19.1",
    "@vitest/ui": "2.1.8",
    "eslint": "9.17.0",
    "eslint-plugin-astro": "1.5.0",
    "prettier": "3.4.2",
    "prettier-plugin-astro": "0.14.1",
    "typescript": "5.7.2",
    "vitest": "2.1.8"
  }
}
