{
  "name": "my-node-server",
  "version": "1.0.0",
  "type": "module",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon --config ./config/nodemon.json app.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config ./config/jest.config.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch --config ./config/jest.config.js",
    "lint": "eslint . --config ./config/eslint.config.js",
    "lint:fix": "eslint . --fix --config ./config/eslint.config.js",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "description": "",
  "dependencies": {
    "cors": "2.8.5",
    "dotenv": "16.4.7",
    "express": "5.2.1",
    "express-session": "1.18.1",
    "pg": "8.13.1"
  },
  "devDependencies": {
    "@eslint/js": "9.17.0",
    "eslint": "9.17.0",
    "globals": "15.14.0",
    "jest": "29.7.0",
    "nodemon": "3.1.9",
    "prettier": "3.4.2"
  }
}
